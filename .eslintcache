nput\"\n              />\n              <br/>\n              <input\n                name=\"email\"\n                value={email}\n                onChange={this.onChange}\n                type=\"text\"\n                placeholder=\"Email Address\"\n                className=\"loginInput\"\n              />\n              <br/>\n              <input\n                name=\"passwordOne\"\n                value={passwordOne}\n                onChange={this.onChange}\n                type=\"password\"\n                placeholder=\"Password\"\n                className=\"loginInput\"\n              />\n              <br/>\n              <input\n                name=\"passwordTwo\"\n                value={passwordTwo}\n                onChange={this.onChange}\n                type=\"password\"\n                placeholder=\"Confirm Password\"\n                className=\"loginInput\"\n              />\n              <br/>\n              <input \n                name=\"accessCode\"\n                value={accessCode}\n                onChange={this.onChange}\n                type=\"number\"\n                placeholder=\"Access Code...\"\n                className=\"loginInput\"\n              />\n              <br/>\n              <button disabled={isInvalid} type=\"submit\" className=\"loginButton\">Sign Up</button>\n       \n              {error && <p>{error.message}</p>}\n            </form>\n        );\n    }\n    }\n    \n    const SignUpLink = () => (\n    <p>\n        Don't have an account? <Link to={ROUTES.SIGNUP}>Sign Up</Link>\n    </p>\n    );\n\nconst SignUpForm = compose(\n    withRouter,\n    withFirebase,\n    )(SignUpFormBase);\n\nexport default SignUpPage;\n \nexport { SignUpForm, SignUpLink };","/Users/jordanschneider1/Documents/GitHub/ebrecipes/ebrecipes/src/components/Session/withAuthorization.js",[],"/Users/jordanschneider1/Documents/GitHub/ebrecipes/ebrecipes/src/components/Session/withAuthentication.js",[],{"ruleId":"146","replacedBy":"147"},{"r